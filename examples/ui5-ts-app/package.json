    "@openui5/ts-types-esm": "1.104.0",
    "@types/jquery": "3.5.14",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "@ui5/cli": "^2.14.10",
    "babel-preset-transform-ui5": "^7.0.5",
    "eslint": "^8.21.0",
    "npm-run-all": "^4.1.5",
    "typescript": "^4.7.4",
    "ui5-middleware-simpleproxy": "^0.9.0"
  },
  "ui5": {
    "dependencies": [
      "ui5-middleware-simpleproxy"
    ]
  }
