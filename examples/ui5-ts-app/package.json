{
  "name": "ui5-t-sapp",
  "version": "0.8.15-notimportant",
  "description": "UI5 Application: tsapp",
  "author": "nosotros-todos@ui5-community.org",
  "license": "Apache-2.0",
  "scripts": {
    "start": "npm-run-all --parallel watch:ts start:ui5",
