{
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "allowJs": true,
    "strict": true,
    "strictPropertyInitialization": false,
    "rootDir": "./webapp",
    "paths": {
      "test/Sample/tsapp/*": ["./webapp/*"]
    },
    "types": ["node", "@wdio/globals/types", "@wdio/mocha-framework", "wdio-ui5-service/esm", "@openui5/types"]
  },
  "include": ["./webapp/**/*"]
}
