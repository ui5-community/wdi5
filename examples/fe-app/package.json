{
  "name": "incidents",
  "version": "0.0.1",
  "description": "SAP Fiori elements",
  "keywords": [
    "ui5",
    "openui5",
    "sapui5"
  ],
  "type": "commonjs",
  "main": "webapp/index.html",
  "scripts": {
    "start": "ui5 serve -p 8088",
    "test": "wait-on tcp:8088 -t 15s && wdio run wdio.conf.js --headless",
    "start:test": "run-p -r start test",
    "deploy": "npx -p @sap/ux-ui5-tooling fiori add deploy-config cf"
  },
  "devDependencies": {
    "@sap-ux/ui5-middleware-fe-mockserver": "latest",
    "wdio-ui5-service": "*"
  }
}
